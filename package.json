{
  "name": "utools-plugin-transfer",
  "version": "1.0.0",
  "description": "utools plugin transfer",
  "keywords": [
    "utools",
    "plugin",
    "electron",
    "react",
    "typescript",
    "ts",
    "sass",
    "webpack"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bougieL/tts-fluent.git"
  },
  "license": "MIT",
  "author": {
    "name": "Bougie Liu",
    "email": "bougie.liu@qq.com",
    "url": "https://bougieblog.cn"
  },
  "main": "./src/main/main.ts",
  "scripts": {
    "build": "npm run build:transfer && concurrently \"npm run build:main\" \"npm run build:renderer\"",
    "build:main": "cross-env NODE_ENV=production TS_NODE_TRANSPILE_ONLY=true webpack --config ./.erb/configs/webpack.config.main.prod.ts",
    "build:renderer": "cross-env NODE_ENV=production TS_NODE_TRANSPILE_ONLY=true webpack --config ./.erb/configs/webpack.config.renderer.prod.ts",
    "build:transfer": "cross-env NODE_ENV=production TS_NODE_TRANSPILE_ONLY=true webpack --config ./.erb/configs/webpack.config.transfer.prod.ts",
    "lint": "cross-env NODE_ENV=development eslint . --ext .js,.jsx,.ts,.tsx",
    "prepare": "husky install",
    "start": "ts-node ./.erb/scripts/check-port-in-use.js && npm run start:renderer",
    "start:main": "cross-env NODE_ENV=development electronmon -r ts-node/register/transpile-only -r tsconfig-paths/register .",
    "start:preload": "cross-env NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true webpack --config ./.erb/configs/webpack.config.preload.dev.ts",
    "start:renderer": "cross-env NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true webpack serve --config ./.erb/configs/webpack.config.renderer.dev.ts",
    "start:transfer": "cross-env NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true webpack serve --config ./.erb/configs/webpack.config.transfer.dev.ts"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "cross-env NODE_ENV=development eslint --cache"
    ],
    "*.json,.{eslintrc,prettierrc}": [
      "prettier --ignore-path .eslintignore --parser json --write"
    ],
    "*.{css,scss}": [
      "prettier --ignore-path .eslintignore --single-quote --write"
    ],
    "*.{html,md,yml}": [
      "prettier --ignore-path .eslintignore --single-quote --write"
    ]
  },
  "browserslist": [],
  "prettier": {
    "singleQuote": true,
    "overrides": [
      {
        "files": [
          ".prettierrc",
          ".eslintrc"
        ],
        "options": {
          "parser": "json"
        }
      }
    ]
  },
  "dependencies": {
    "@bougiel/tts-node": "^1.2.5",
    "@fluentui/font-icons-mdl2": "^8.4.2",
    "@fluentui/react": "^8.78.0",
    "address": "^1.2.0",
    "axios": "^0.27.2",
    "cookie-parser": "^1.4.6",
    "copy-to-clipboard": "^3.3.2",
    "electron": "18.2.3",
    "express": "^4.18.1",
    "fs-extra": "^10.1.0",
    "md5": "^2.3.0",
    "multer": "^1.4.5-lts.1",
    "qrcode": "^1.5.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-dropzone": "^14.2.2",
    "react-router-dom": "^6.3.0",
    "react-toastify": "^9.0.7",
    "react-use": "^17.4.0",
    "utools-api-types": "^3.0.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.6",
    "@teamsupercell/typings-for-css-modules-loader": "^2.5.1",
    "@types/cookie-parser": "^1.4.3",
    "@types/detect-port": "^1.3.2",
    "@types/fs-extra": "^9.0.13",
    "@types/md5": "^2.3.2",
    "@types/multer": "^1.4.7",
    "@types/node": "17.0.33",
    "@types/qrcode": "^1.4.2",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "@types/terser-webpack-plugin": "^5.0.4",
    "@types/uuid": "^8.3.4",
    "@types/webpack-bundle-analyzer": "^4.4.1",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "browserslist-config-erb": "^0.0.3",
    "chalk": "^4.1.2",
    "concurrently": "^7.1.0",
    "core-js": "^3.22.5",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "detect-port": "^1.3.0",
    "eslint": "^8.15.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-erb": "^4.0.3",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-compat": "^4.0.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.5.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.1",
    "identity-obj-proxy": "^3.0.0",
    "lint-staged": "^12.4.1",
    "mini-css-extract-plugin": "^2.6.0",
    "node-sass": "^7.0.1",
    "prettier": "^2.6.2",
    "react-refresh": "^0.13.0",
    "rimraf": "^3.0.2",
    "sass": "^1.51.0",
    "sass-loader": "^12.6.0",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.1",
    "ts-loader": "^9.3.0",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.6.4",
    "url-loader": "^4.1.1",
    "vconsole": "^3.14.6",
    "webpack": "^5.72.1",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.9.0",
    "webpack-merge": "^5.8.0"
  }
}
